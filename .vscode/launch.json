{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome (dev)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/Portfolio/src",
      "preLaunchTask": "dev",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    },
    {
      "name": "Attach to Chrome",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}/Portfolio/src",
      "sourceMaps": true
    },
    {
      "name": "Launch Edge (dev)",
      "type": "msedge",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/Portfolio/src",
      "preLaunchTask": "dev",
      "sourceMaps": true
    }
  ]
}
